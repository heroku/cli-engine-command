{
  "name": "cli-engine-command",
  "description": "base CLI command for cli-engine",
  "version": "3.0.1",
  "author": "Jeff Dickey @dickeyxxx",
  "bugs": "https://github.com/heroku/cli-engine-command/issues",
  "dependencies": {
    "ansi-escapes": "^1.4.0",
    "cardinal": "^1.0.0",
    "chalk": "^1.1.3",
    "cli-engine-config": "1.0.3",
    "fs-extra": "2.1.2",
    "got": "^6.7.1",
    "http-call": "^1.1.0",
    "jest-validate": "^19.0.2",
    "lodash.maxby": "^4.6.0",
    "netrc-parser": "^2.0.1",
    "pretty-format": "^19.0.0",
    "string": "^3.3.3",
    "supports-color": "^3.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "flow-bin": "0.43.1",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "19.0.2",
    "jest-junit": "1.4.0",
    "nock": "9.0.13",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "standard": "10.0.1",
    "std-mocks": "1.0.1"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/heroku/cli-engine-command",
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "coveragePathIgnorePatterns": [
      "src/output/linewrap.js"
    ],
    "setupFiles": [
      "../test/init.js"
    ]
  },
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "repository": "heroku/cli-engine-command",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '**/*.test.js' src lib",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "test": "jest && flow && standard",
    "watch": "nodemon -e js -i lib --exec \"npm run prepare\""
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "globals": [
      "$Shape",
      "$PropertyType",
      "$Enum",
      "stream$Writable",
      "Class"
    ],
    "ignore": [
      "lib",
      "src/output/linewrap.js",
      "flow-typed"
    ]
  }
}
